{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist && swc ./src -d dist",
    "start": "node dist/src/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"node -r @swc/register ./src/index.ts\"",
    "docker:db:migrate": "npx prisma migrate dev",
    "docker:seed:dev": "ts-node ./scripts/seed/database.ts",
    "start:migrate:prod": "npx prisma migrate deploy && npx prisma generate && npm start",
    "rebuild:be": "docker compose -f docker-compose.dev.yml build api && docker compose -f docker-compose.dev.yml rm --force --stop api && docker compose -f docker-compose.dev.yml up -d api"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "body-parser": "^1.20.3",
    "express": "^4.20.0",
    "joi": "^17.13.3",
    "morgan": "^1.10.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.25",
    "@swc/register": "^0.1.10",
    "@tsconfig/node20": "^20.1.4",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "eslint": "^9.10.0",
    "nodemon": "^3.1.4",
    "prisma": "^5.19.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "watchy": "^0.10.2"
  }
}
